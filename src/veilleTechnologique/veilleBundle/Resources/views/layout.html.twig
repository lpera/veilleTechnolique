{% block head %}
    <meta charset="utf-8">

    <title> Home </title>
    {% block stylesheets %}
        <link href="{{ asset('bundles/veilleTechnologique/css/bootstrap.min.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/veilleTechnologique/css/jumbotron.css') }}" rel="stylesheet">
    {% endblock %}
{% endblock %}

{% block body %}
    
        <nav class="navbar navbar-inverse navbar-fixed-top">
            <div class="container">
            <div class="navbar-header">
              <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
              </button>
              <a class="navbar-brand" href="{{ path('_home_hello', {}) }}">Veille Technologique</a>
            </div>
            <div id="navbar" class="navbar-collapse collapse">
              <form class="navbar-form navbar-right">
                <div class="form-group">
                  <input type="text" placeholder="Email" class="form-control">
                </div>
                <div class="form-group">
                  <input type="password" placeholder="Password" class="form-control">
                </div>
                 <a  class="btn btn-primary">connexion</a>
                 <a href="{{ path('_home_inscription', {}) }}" class="btn btn-success">inscription</a>
              </form>
            </div><!--/.navbar-collapse -->
          </div>
        </nav>
        
        <div class="jumbotron">
            <div class="container">
                <h1>Hello, world!</h1>
                <p>This is a template for a simple marketing or informational website. It includes a large callout called a jumbotron and three supporting pieces of content. Use it as a starting point to create something more unique.</p>
                <p><a class="btn btn-primary btn-lg" href="#" role="button">Learn more &raquo;</a></p>
            </div>
        </div>
        
        <div class="block">
            {% block content %}{% endblock %}
        
      </div>
      {% set flashbag_notices = app.session.flashbag.get('notice')%}  
        
        
        <script type="text/javascript" src="{{ asset('bundles/veilleTechnologique/js/jquery.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/veilleTechnologique/js/bootstrap.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/veilleTechnologique/js/fonction.js') }}"></script>
        
{% endblock %}